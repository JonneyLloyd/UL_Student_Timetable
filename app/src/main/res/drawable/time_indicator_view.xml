            <solid
                android:color="#FF1166"/>
        </shape>
    </item>
    <item android:height="2dp" android:top="5dp" android:left="6dp">
        <shape android:shape="rectangle">
            <solid
                android:color="#FF1166"/>
        </shape>
    </item>
</layer-list>